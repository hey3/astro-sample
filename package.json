{
  "name": "astro-sample",
  "description": "Astro sample repository",
  "version": "1.0.0",
  "author": "hey3",
  "keywords": [
    "astro"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/hey3/astro-sample"
  },
  "bugs": {
    "url": "https://github.com/hey3/astro-sample/issues"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier . --check --plugin-search-dir=. --ignore-unknown",
    "format:fix": "prettier . --write --plugin-search-dir=. --ignore-unknown",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --fix --ext .js,.ts,.astro",
    "typecheck": "astro check && tsc --project tsconfig.json --pretty --noEmit --incremental false"
  },
  "dependencies": {
    "astro": "1.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "astro-eslint-parser": "0.9.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-astro": "0.21.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-astro": "0.7.0"
  }
}
