---
import Footer from '../components/Footer.astro'
import Header from '../components/Header.astro'
import GlobalLayout from './GlobalLayout.astro'

export type Props = {
  title?: string
}

const { title } = Astro.props
---

<GlobalLayout title={title}>
  <Header slot="header" />
  <slot />
  <Footer slot="footer" />
</GlobalLayout>
